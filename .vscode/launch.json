    "version": "0.2.0",
    "showDevDebugOutput": true,
    "configurations": [
        {
            "name": "Debug prx",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "external",
            "gdbTarget": "host.docker.internal:2331",
            "svdFile": "modules/hal/nordic/nrfx/mdk/nrf54l15_application.svd",
            "cwd": "${workspaceFolder}",
            "executable": "esb_prx/build/merged.hex",
            "interface": "swd",
            "device": "nRF54L15_M33",
            "overrideLaunchCommands": [
                "monitor reset 0",
                "-target-download",
                "monitor reset 0"
            ],
            "postLaunchCommands": [
                "file esb_prx/build/esb_prx/zephyr/zephyr.elf"
            ],
            "overrideResetCommands": [
                "monitor reset 0"
            ],
            "rtos": "Zephyr",
            "showDevDebugOutput": "none",
            "runToEntryPoint": "main"
        },
        {
            "name": "Debug ptx",
            "type": "cortex-debug",
